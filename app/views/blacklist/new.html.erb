<div class="crumbs">
  <%= link_to "Forums", root_path() %> <span class="arrow">&rarr;</span>
</div>

<h1>New Blacklist</h1>

<%= form_for :blacklist, url: blacklist_path() do |f| %>
  <%= render :partial => "form", :object => f %>
  <%= submit_tag 'Create' %> or <%= link_to('Cancel', blacklist_path()) %>
<% end %>
