<div class="crumbs">
  <%= link_to "Forums", root_path() %> <span class="arrow">&rarr;</span>
</div>

<h1>Edit Blacklist</h1>

<%= form_for :blacklist, url: blacklist_path(), html: { method: :put } do |f | %>
  <%= render :partial => "form", :object => f %>
  <%= submit_tag 'Save' %> or <%= link_to('Cancel', blacklist_path()) %>
<% end %>
